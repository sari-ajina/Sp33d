<br/>\r\n                <input \r\n                    type=\"text\" \r\n                    placeholder=\"i.e. Doe\"\r\n                    onChange={((e)=>\r\n                        {setLastNameReg(e.target.value)}\r\n                    )}\r\n                    required\r\n                >\r\n                </input>\r\n            </div>\r\n            <div>\r\n                Enter Username: <br/>\r\n                <input \r\n                    type=\"text\" \r\n                    placeholder=\"i.e. johndoe\"\r\n                    onChange={((e)=>\r\n                        {setUsernameReg(e.target.value)}\r\n                    )}\r\n                    required\r\n                >\r\n                </input>\r\n            </div>\r\n            <div>\r\n                Enter Email: <br/>\r\n                <input \r\n                    placeholder=\"i.e. johndoe@email.com\"\r\n                    type=\"text\" \r\n                    onChange={((e)=>\r\n                        {setEmailReg(e.target.value)}\r\n                    )}\r\n                    required\r\n                >\r\n                </input>\r\n            </div>\r\n            <div>\r\n                Enter Password: <br/>\r\n                <input \r\n                    placeholder=\"password\"\r\n                    type=\"text\" \r\n                    onChange={((e)=>\r\n                        {setPasswordReg(e.target.value)}\r\n                    )}\r\n                    required\r\n                >\r\n                </input>\r\n            </div>\r\n            <div>\r\n                Confirm Password: <br/>\r\n                <input \r\n                    placeholder=\"input the same password\"\r\n                    type=\"text\" \r\n                    onChange={((e)=>\r\n                        {setPasswordConf(e.target.value)}\r\n                    )}\r\n                    required\r\n                >\r\n                </input>\r\n            </div>\r\n   